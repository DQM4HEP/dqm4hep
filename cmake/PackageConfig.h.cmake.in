#ifndef @PACKAGE_CONFIG_PACKAGE_NAME@Config_H
#define @PACKAGE_CONFIG_PACKAGE_NAME@Config_H

// version macros
#define @PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MAJOR @PACKAGE_CONFIG_PACKAGE_VERSION_MAJOR@
#define @PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MINOR @PACKAGE_CONFIG_PACKAGE_VERSION_MINOR@
#define @PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_PATCH @PACKAGE_CONFIG_PACKAGE_VERSION_PATCH@

#define @PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_STR "@PACKAGE_CONFIG_PACKAGE_VERSION@"

#define @PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_GE( MAJV , MINV , PLEV )  \
     ( (@PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MAJOR > MAJV) || ( (@PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MAJOR == MAJV) && (@PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MINOR > MINV) ) \
  || ( (@PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MAJOR == MAJV) && (@PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_MINOR == MINV) && (@PACKAGE_CONFIG_PACKAGE_NAME@_VERSION_PATCH >= PLEV) ) )

// project source dir
#define @PACKAGE_CONFIG_PACKAGE_NAME@_DIR "@CMAKE_INSTALL_PREFIX@"

#endif // @PACKAGE_CONFIG_PACKAGE_NAME@Config_H
